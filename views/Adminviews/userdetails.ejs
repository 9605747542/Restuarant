							<div class="content-title">
								<i><a href="/dashboard" style="font-size: 25px;">Back</a></i>
								<div class="cpa">
									<i class="fa-solid fa-file-lines me-1">User List</i>
								</div>
								<div class="tools">
									<a href="javascript:void(0);" class="expand SlideToolHeader"><i class="fal fa-angle-down"></i></a>
								</div>
							</div>
							<div class="cm-content-body form excerpt">
								<div class="card-body pt-2">
									<div class="table-responsive">
										
										<table class="table table-responsive-sm mb-0">
											<thead>
												<tr>
													<th>
														<div class="form-check">
														  <input class="form-check-input" type="checkbox" value="" id="checkAll">
														  <label class="form-check-label" for="checkAll">
														   
														  </label>
														</div>
													</th>
													<th><strong>S.No</strong></th>
													<th><strong>Name</strong></th>
													<th><strong>Email</strong></th>
													<th><strong>Status</strong></th>
													<th style="width:85px;"><strong>Actions</strong></th>
												</tr>
											</thead>
											<tbody>
												
													<% for(let i = data.length-1; i >=0; i--) { %>
														<tr>
														<td>
															<div class="form-check">
																<input class="form-check-input" type="checkbox" value="" id="flexCheckDefault-<%= i + 1 %>">
																<label class="form-check-label" for="flexCheckDefault-<%= i + 1 %>"></label>
															</div>
														</td>
														<td><b><%= i + 1 %></b></td>
														<td><%= data[i].name %></td>
														<td><%= data[i].email %></td>
														<td><%= data[i]. isBlocked ? 'Blocked' : 'Active' %></td>
														<td>
														<% if (data[i].isBlocked==true) { %>
															<a  href="/adminuserunblock/<%= data[i]._id%>">Unblock</a>
														  <% } else { %>
															<a onclick="confirmBlock()" class="text-danger" href="/adminuserblock/<%= data[i]._id%>">Block</a>
														  <% } %>
														</td>

													</tr>
													<% } %>
												
												

											</tbody>
										</table>
											
									</div>
								
								</div>
							</div>
						</div>
