    <!-- Required vendors -->


		<script>
			function confirmBlock() {
            const result = confirm("Are you sure you want to block the User?");

            if (!result) {
                // The user clicked "Cancel," so prevent the form from submitting
                event.preventDefault();
            }
            // If the user clicked "OK," the form will be submitted as usual
        }
		</script>

